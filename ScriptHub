--HUMANOID TAB
local HumanoidTAB = Window:NewTab("Humanoid")
local HumanoidSECTION = HumanoidTAB:NewSection("HUMANOID/SPEED")
HumanoidSECTION:NewButton("Sit", "Makes you sit", function()
    game.Players.LocalPlayer.Character.Humanoid.Sit = true
	game:GetService("StarterGui"):SetCore("SendNotification",{
            Title = "BOSS HUB";
            Text = "Done";
                    })
end)

HumanoidSECTION:NewSlider("Speedandjump", "600 = max", 600, 16, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
	game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
		game:GetService("StarterGui"):SetCore("SendNotification",{
            Title = "BOSS HUB";
            Text = "Done";
                    })
end)
HumanoidSECTION:NewButton("Set normal speed", "[Content Deleted]", function()
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
	game:GetService("StarterGui"):SetCore("SendNotification",{
            Title = "BOSS HUB";
            Text = "Done";
                    })
end)
HumanoidSECTION:NewButton("Set JumpPower", "[Content Deleted]", function()
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
	game:GetService("StarterGui"):SetCore("SendNotification",{
            Title = "BOSS HUB";
            Text = "Done";
                    })
end)

